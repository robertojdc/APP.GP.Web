</head>
<body>
    <div class="container nav-header-container">
        <div class="col-auto header-content">
            <a href="#" title="Consejo Nacional Morena" class="logo"></a>
        </div>
    </div>

    <div class="container-fluid">
        @RenderBody()
    </div>

    @await RenderSectionAsync("Scripts", required: false)
